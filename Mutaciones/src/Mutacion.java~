public class Mutacion {

    String nombreMutacion;
    VariacionGenetica variacion;

    public Mutacion(String nombreMutacion) {
        this.nombreMutacion = nombreMutacion;
    }

    void describirEfecto() {
        System.out.println(nombreMutacion + " es una mutaci√≥n.");
        variacion.describirEfecto();
    }


}
